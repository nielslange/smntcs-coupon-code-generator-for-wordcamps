{
	"name": "nielslange/smntcs-coupon-code-generator-for-wordcamps",
	"description": "This plugin generates coupon codes for WordCamp tickets.",
	"homepage": "https://wordpress.org/plugins/smntcs-coupon-code-generator-for-wordcamps/",
	"type": "wordpress-plugin",
	"authors": [
		{
			"name": "Niels Lange",
			"email": "info@nielslange.de"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"humanmade/psalm-plugin-wordpress": "^2.0",
		"overtrue/phplint": "^2.3",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"squizlabs/php_codesniffer": "^3.6",
		"vimeo/psalm": "^4.7",
		"wp-coding-standards/wpcs": "^2.3"
	},
	"scripts": {
		"phpcbf": "phpcbf --standard=phpcs.dist.xml .",
		"phpcs": "phpcs --standard=phpcs.dist.xml .",
		"phplint": "phplint",
		"psalm": "psalm --no-cache"
	},
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
